 <root main_tree_to_execute = "MainTree" >
     <BehaviorTree ID="MainTree">
        <ReactiveSequence name="root_sequence">
            <!--GetPosition name="get_position" goal="2" position="{position}"/>
            <GoToPosition   name="move" position="{position}"/-->
            <ReactiveFallback>
                <DetectPerson name="detect_person"/>
                <Turn name="turn"/>
            </ReactiveFallback>
            <ApproachPerson name="approach_person"/>
        </ReactiveSequence>
     </BehaviorTree>
 </root>