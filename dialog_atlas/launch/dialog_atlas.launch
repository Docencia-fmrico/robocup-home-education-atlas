<launch>
    <arg name="config_file" default="$(find dialog_atlas)/config/params.yaml"/>
    <arg name="google_application_credentials" default="$(env HOME)/obtener_nombre.json"/>

    <node pkg="dialog_atlas" type="get_name_node" name="Get_Name" output="screen"></node>
    <node name="soundplay_node" pkg="sound_play" type="soundplay_node.py" output="screen"/>
    <include file="$(find dialogflow_ros)/launch/dialogflow.launch" >
        <arg name="google_application_credentials" value="$(arg google_application_credentials)"/>
        <arg name="config_file" value="$(arg config_file)"/>
  </include>
</launch>